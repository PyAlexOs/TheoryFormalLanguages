dim sum, i, auxiliary %
dim printFalse !
printFalse as 0.0E1000 {для вывода неверного результата}

for i as 1 to i <= 10 do [
    sum as sum + i
    while auxiliary > 0 do [
        auxiliary as auxiliary - 2
    ]

    if auxiliary = 0 then write(auxiliary)
    else write(printFalse)

    i as i + 1d]

write(sum)
end